generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_PRISMA_URL")
}

model Build {
  auth0Owner       String?
  email            String?
  buildID          String  @id
  buildName        String?
  orokinReactor    Boolean @default(true)
  itemRank         Int     @default(30)
  buildType        String?
  assignedMods     Json
  slotPolarities   Json
  currentModRanks  Json
  selectedWarframe Json?
  selectedWeapon   Json?
}
